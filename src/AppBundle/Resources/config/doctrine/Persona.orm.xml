<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Persona" table="persona">
    <id name="idpersona" type="integer" column="IDPERSONA">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="NOMBRE" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="apellidos" type="string" column="APELLIDOS" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechanacimiento" type="date" column="FECHANACIMIENTO" nullable="true"/>
    <field name="dui" type="string" column="DUI" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nit" type="string" column="NIT" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="pasaporte" type="string" column="PASAPORTE" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="telefono" type="string" column="TELEFONO" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="correo" type="string" column="CORREO" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="sexo" type="string" column="SEXO" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="direccion" type="string" column="DIRECCION" length="250" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
