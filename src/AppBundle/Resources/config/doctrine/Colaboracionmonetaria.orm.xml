<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Colaboracionmonetaria" table="colaboracionmonetaria">
    <id name="idcolaboracionmonetaria" type="integer" column="IDCOLABORACIONMONETARIA">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="montomes" type="decimal" column="MONTOMES" precision="10" scale="0" nullable="true"/>
    <field name="descripcion" type="string" column="DESCRIPCION" length="100" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="saldo" type="decimal" column="SALDO" precision="10" scale="0" nullable="true"/>
    <field name="estado" type="smallint" column="ESTADO" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
