<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Publicacioncontenido" table="publicacioncontenido">
    <id name="idcontenido" type="integer" column="IDCONTENIDO">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="titulo" type="string" column="TITULO" length="250" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="subtitulo" type="string" column="SUBTITULO" length="250" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="descripcion" type="string" column="DESCRIPCION" length="250" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechapublicacion" type="date" column="FECHAPUBLICACION" nullable="true"/>
    <field name="estado" type="smallint" column="ESTADO" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
